{
  "name": "KentSearch",
  "description": "Get to any class page quickly! Try typing 'Kent' in your omnibox/address bar or clicking the falcon in your toolbar (next to the address bar) to begin.",
  "omnibox": {
    "keyword": "kent"
  },
  "permissions": [
    "storage"
   ],
  // "options_page": "options.html", (removed for v0.5 release)
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "newtab.html"
  },
  "icons": {
     "16": "icon.png",
     "48": "icon.png",
     "128": "icon.png"
  },
  "version": "0.5.1",
  "minimum_chrome_version": "44",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://www.google.com/*"],
      "js": ["newtab.js"]
    }
  ],
  "web_accessible_resources": [
    "newtab.html",
    "popup.js",
    "icon.png",
    "options.html"
  ]
}
